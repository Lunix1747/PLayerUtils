-- Scripts

local function OIMIGXO_fake_script() -- AntiBring.anti bring 
	local script = Instance.new('LocalScript', AntiBring)

	local b = script.Parent
	
	b.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Lunix1747/PLayerUtils/refs/heads/main/Anti%20Tool%20Bring"))()
	end)
end
coroutine.wrap(OIMIGXO_fake_script)()
